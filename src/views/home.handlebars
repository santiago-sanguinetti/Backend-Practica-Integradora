<div class="container">
  <h1 class="text-center">Products List</h1>
  <div class="row">
    {{#each products}}
      <div class="col-md-4">
        <div class="card mb-4">
          <img src="{{this.thumbnails}}" class="card-img-top" alt="{{this.title}}">
          <div class="card-body">
            <h5 class="card-title">{{this.title}}</h5>
            <p class="card-text">Code: {{this.code}}</p>
          </div>
        </div>
      </div>
    {{/each}}
  </div>
</div>
